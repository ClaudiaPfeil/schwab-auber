<div class="tr span-22">
  
  <div class="td first">&nbsp;</div>
  <div class="td span-22">
    <h3><%= I18n.t(:profiles_title)%> </h3>

    <%= semantic_form_for @profile do |profile| %>

      <%= render "user", :profile => profile %>

      <%= render "settings", :profile => profile %>


      <%= profile.buttons %>

    <% end %>
  </div>
  <div class="td last">&nbsp;</div>
 
</div>


